<p>Please Enter a New Creature (Unless it's Ugly)</p>

<form action="/creatures" method="post">
	<label>Name: </label><br>
	<input type="text" name="creature[name]"><br>
	<label>Description: </label><br>
	<textarea name="creature[description]"></textarea><br>
	<%= token_tag form_authenticity_token %>
	<button>Save Creature</button>
</form>